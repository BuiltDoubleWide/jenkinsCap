doctype 
html
    head
        link(href='style.css' rel='stylesheet')
        title='TheBoxingCo'
        body
            header
                nav 
                    ul 
                        each item in config.menu 
                            a(href=item[1] target=item[2])
                                div(class='menu_item')
                                    li(id=item[0]) #{item[0]}
            section 
                h1 #{title}
                block content 
            footer